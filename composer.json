{
    "name": "durableprogramming/durable-laravel-typst",
    "description": "Laravel facade plugin for Typst document compilation",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Durable",
            "email": "info@durable.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Durable\\LaravelTypst\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Durable\\LaravelTypst\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Durable\\LaravelTypst\\TypstServiceProvider"
            ],
            "aliases": {
                "Typst": "Durable\\LaravelTypst\\Facades\\Typst"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
